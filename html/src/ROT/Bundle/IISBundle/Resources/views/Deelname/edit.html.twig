{% extends "ROTIISBundle::layout.html.twig" %}
{% block title %}Wijzig deelname "{{deelname}}"{% endblock %}
{% block inner_content %}
    {{ include('ROTIISBundle:Deelname:_object_actions.html.twig', {"deelname": deelname}) }}
    <form novalidate action="{{path('rot_iis_deelname_update', {'id': deelname.id})}}" method="POST" {{form_enctype(form)}}>
        {{ include('ROTIISBundle:Deelname:_form.html.twig', {'form': form}) }}
        {{ form_rest(form) }}
        <input type="hidden" name="_method" value="PUT" />
        <input type="submit" value="Opslaan" />
    </form>
{% endblock %}