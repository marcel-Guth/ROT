{% set route = app.request.attributes.get('_route') %}
<ul class="buttons_large">
    <li><a href="{{path('rot_iis_nationaliteit_index')}}">Index</a></li>
    {% if route != 'rot_iis_nationaliteit_new' %}
        {% if route != 'rot_iis_nationaliteit_show' %}
            <li><a href="{{path('rot_iis_nationaliteit_show', {'id': nationaliteit.id})}}">Bekijk</a></li>
        {% endif %}
        {% if is_granted('ROLE_WIJZIG_IIS') %}
            {% if route != 'rot_iis_nationaliteit_edit' %}
                <li><a href="{{path('rot_iis_nationaliteit_edit', {'id': nationaliteit.id})}}">Wijzig</a></li>
            {% endif %}
            <li><a href="{{path('rot_iis_nationaliteit_delete', {'id': nationaliteit.id})}}" data-method="DELETE">Verwijder</a></li>
        {% endif %}
        {% if route != 'rot_iis_nationaliteit_log' %}
            <li><a href="{{ path('rot_iis_nationaliteit_log', {'id': nationaliteit.id}) }}">Log</a></li>
        {% endif %}
    {% endif %}
</ul>