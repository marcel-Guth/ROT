{% extends "ROTAdminBundle::admin_layout.html.twig" %}
{% block title %}Nieuwe gebruiker{% endblock %}
{% block inner_content %}
    {{ include('ROTAdminBundle:Gebruiker:_object_actions.html.twig') }}
    <form novalidate action="{{path('rot_admin_gebruiker_create')}}" method="POST" {{form_enctype(form)}}>
        {{ include('ROTAdminBundle:Gebruiker:_form.html.twig', {'form': form}) }}
        {{ form_rest(form) }}
        <input type="submit" value="Aanmaken" />
    </form>
{% endblock %}