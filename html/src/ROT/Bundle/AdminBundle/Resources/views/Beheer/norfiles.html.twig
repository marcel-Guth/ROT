{% extends "ROTAdminBundle::beheer_layout.html.twig" %}
{% block title %}NOR{% endblock %}
{% block inner_content %}
    <h3>Selecteer actieve bestanden</h3>
    <form action={{ path('rot_admin_beheer_selectnorfiles') }} {{ form_enctype(select_form) }}>
        {{ form_rest(select_form) }}
        <input type="submit" value="Opslaan" />
    </form>
    <h3>Upload nieuw bestand</h3>
    <form action={{ path('rot_admin_beheer_uploadnorfile') }} {{ form_enctype(upload_form) }}>
        {{ form_rest(upload_form) }}
        <input type="submit" value="Uploaden" />
    </form>
{% endblock %}
